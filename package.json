  "bugs": {
    "url": "https://github.com/Recras/online-booking-js/issues"
  },
  "homepage": "https://github.com/Recras/online-booking-js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "jshint": "^2.9.5",
    "pikaday": "^1.7.0"
  }
